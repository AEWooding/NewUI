<ext4:container rootPath="/vista/ext-4" onBeforeRender="getChartData^VitalsDemo01">

  <ext4:panel title="Column Chart" >

    <ext4:chart id="myFirstChart" sessionName="myChart" width="500" height="300">

      <ext4:axis type="Numeric" position="left" fields="line1" title="Score" grid="true" minimum="0" />
      <ext4:axis type="Category" position="bottom" fields="month" grid="true" title="Month" />

      <ext4:series type="column" axis="left" xField="month" yField="line1" />

    </ext4:chart>

  </ext4:panel>

</ext4:container>
