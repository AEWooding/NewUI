<ext4:container rootPath="/vista/ext-4" onBeforeRender="getChartData^VitalsDemo01">
  <ext4:panel title="Bar Chart" layout="fit" height="400" width="500">
    <ext4:chart sessionName="myChart" animate="true" flex="1">
      <ext4:axis type="Numeric" position="bottom" fields="line1" title="Score" grid="true" minimum="0" />
      <ext4:axis type="Category" position="left" fields="month" grid="true" title="Month" />
      <ext4:series type="bar" axis="left" xField="month" yField="line1" />
    </ext4:chart>
  </ext4:panel>
</ext4:container>
