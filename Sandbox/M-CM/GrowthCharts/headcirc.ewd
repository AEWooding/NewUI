<ext4:fragment onBeforeRender="getHeadCircData^growthChartsMCM">

  <ext4:chart sessionName="headcircChart" animate="true" flex="1">
    <ext4:axis type="Numeric" position="left"   title="Head Circumference" field="HeadCirc" grid="true" minimum="0" maximum="60" />
    <ext4:axis type="Time"    position="bottom" title="Date"               field="Date"     grid="true" />
    <ext4:series type="line" axis="left" xField="Date" yField="HeadCirc" />
  </ext4:chart>

</ext4:fragment>

