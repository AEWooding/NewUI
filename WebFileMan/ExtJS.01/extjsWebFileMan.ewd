<ext4:container rootPath="/vista/ext-4" onBeforeRender="getTreeMenu^extjsWebFileMan">
  <ext4:viewPort layout="border">
    <ext4:panel region="north" id="northPanel" authoheight="true" border="false" margins="0 0 5 0" html="<h1 class='x-panel-header'>Welcome to Web FileMan</h1>" />
    <ext4:panel region="west" collapsible="true" title="Navigation" width="450" height="650" addPage="layout3Tree" />
    <ext4:panel region="south" collapsible="true" title="South Panel" split="true" height="100" minHeight="100" html="This is the South Panel" />
    <ext4:panel region="center" activeTab="0">
      <ext4:panel title="Default Tab" id="centerPanel" />
    </ext4:panel>
  </ext4:viewPort>
</ext4:container>
